bool isPrime(int N)
{
    //Your code here
    int flag=0;
    for(int i=2;i<=sqrt(N);i++)
    {
        if(N%i==0)
        {
        flag=1;
        return false;
        }
    }
    if(flag==0)
    return true;

}
