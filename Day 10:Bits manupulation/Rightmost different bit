int posOfRightMostDiffBit(int m, int n)
    {
          int x;
        int ans=0;
        int i=1;
        while(a!=0 || b!=0)
        {
            x=(a^b);
            if((x&1)==1)
            return i;
            a=a>>1;
            b=b>>1;
            i++;
        }
      return i;  
    }
